{
  "name": "cot-experience",
  "version": "0.9.4-6",
  "description": "Capital On Tap Experience is an end to end design language to create application's at Capital On Tap.",
  "main": "index.js",
  "repository": "https://github.com/CapitalOnTap/COTE",
  "author": "Ricardo Abreu <ricardo.abreu@capitalontap.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --port 5000",
    "docs": "NODE_ENV=production webpack -p",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "babel src -d dist",
    "clean": "rimraf dist",
    "dist": "yarn run clean && yarn run build",
    "export-storybook": "build-storybook -c .storybook -o .out-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "docker-storybook": "docker build -t storybook . && docker run -it storybook",
    "start-storybook": "serve  .out-storybook -p 5000"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-info": "^3.3.9",
    "@storybook/addon-knobs": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.9",
    "global": "^4.3.2",
    "html-webpack-plugin": "^2.30.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "serve": "^9.1.0",
    "text-mask-addons": "^3.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.9",
    "react-text-mask": "5.4.1",
    "react-tippy": "^1.2.2",
    "styled-components": "^3.3.0"
  },
  "peerDependencies": {
    "styled-components": ">=2.0 || >= 3.0"
  }
}
