{
  "name": "cot-experience",
  "version": "0.14.10",
  "description": "Capital On Tap Experience is an end to end design language to create applications at Capital On Tap.",
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "jsnext:main": "dist/main.es.js",
  "repository": "https://github.com/CapitalOnTap/COTE",
  "author": "Ricardo Abreu <ricardo.abreu@capitalontap.com>",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "start": "webpack-dev-server --port 5000",
    "docs": "set NODE_ENV=production webpack -p",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "yarn clean && yarn build-bundle && yarn build-packages",
    "build-bundle": "rollup -c",
    "build-packages": "babel src/ --out-dir lib/ --copy-files",
    "clean": "rimraf lib && rimraf dist",
    "dist": "set NODE_ENV=production && yarn run clean && yarn run build",
    "export-storybook": "build-storybook -c .storybook -o .out-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "docker-storybook": "docker build -t storybook . && docker run -it storybook",
    "start-storybook": "serve  .out-storybook -p 5000"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/addon-knobs": "^4.0.0",
    "@storybook/addon-options": "^4.1.6",
    "@storybook/react": "^4.0.0",
    "@storybook/storybook-deployer": "^2.2.0",
    "@types/jest": "^23.3.10",
    "@types/react-dropzone": "4.2.2",
    "@types/react-text-mask": "5.4.1",
    "@types/storybook-addon-jsx": "^5.4.0",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.5.2",
    "react-dropzone": "4.2.3",
    "react-ripples": "^1.1.2",
    "react-text-mask": "5.4.1",
    "react-tippy": "^1.2.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-visualizer": "^0.9.0",
    "serve": "^9.1.0",
    "styled-components": "^4.1.3",
    "text-mask-addons": "^3.7.1",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "webpack": "^4.0.0"
  },
