  "scripts": {
    "start": "webpack-dev-server --port 5000",
    "docs": "set NODE_ENV=production webpack -p",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "webpack -p",
    "clean": "rimraf dist",
    "dist": "set NODE_ENV=production && yarn run clean && yarn run build",
    "export-storybook": "build-storybook -c .storybook -o .out-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "docker-storybook": "docker build -t storybook . && docker run -it storybook",
    "start-storybook": "serve  .out-storybook -p 5000"
  },
