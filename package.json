    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "html-webpack-plugin": "^2.30.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "text-mask-addons": "^3.7.1",
