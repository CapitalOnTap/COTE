{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "css-modules-transform",
      {
        "extractCss": "./dist/stylesheets/styles.css"
      }
    ]
  ]
}
